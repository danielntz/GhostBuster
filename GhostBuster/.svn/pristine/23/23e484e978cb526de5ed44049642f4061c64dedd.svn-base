package com.example.data;

public class Hero {
               
	       private     String  name;
	       private     int     age;
	       private    int      dengji;
	       private    int     attack;      //攻击力, 消灭灵魂所需要的次数
	       private   int      defend ;  //防御力,  增加通关时间
	       private   int      zidanshu;  //剩余子弹数
	       private  Weapon   weapon;    //武器种类
	       private  bullet           bulletkind ;  //子弹种类
		public String getName() {
			return name;
		}
		public void setName(String name) {
			this.name = name;
		}
		public int getAge() {
			return age;
		}
		public void setAge(int age) {
			this.age = age;
		}
		public int getDengji() {
			return dengji;
		}
		public void setDengji(int dengji) {
			this.dengji = dengji;
		}
		public int getAttack() {
			return attack;
		}
		public void setAttack(int attack) {
			this.attack = attack;
		}
		public int getDefend() {
			return defend;
		}
		public void setDefend(int defend) {
			this.defend = defend;
		}
		public int getZidanshu() {
			return zidanshu;
		}
		public void setZidanshu(int zidanshu) {
			this.zidanshu = zidanshu;
		}
		public Weapon getWeapon() {
			return weapon;
		}
		public void setWeapon(Weapon weapon) {
			this.weapon = weapon;
		}
		public bullet getBulletkind() {
			return bulletkind;
		}
		public void setBulletkind(bullet bulletkind) {
			this.bulletkind = bulletkind;
		}
	       
	
}
